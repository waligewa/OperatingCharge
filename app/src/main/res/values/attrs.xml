<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义组件 -->
    <declare-styleable name="TabContainerViewStyle">
        <attr name="tabTextSize" format="dimension" />
        <attr name="tabTextColor" format="color" />
        <attr name="selectedTextColor" format="color" />
        <attr name="drawablePadding" format="dimension" />
        <attr name="iconHeight" format="dimension" />
        <attr name="iconWidth" format="dimension" />
        <attr name="divideLineColor" format="color" />
        <attr name="divideLineHeight" format="dimension" />
    </declare-styleable>
    <!--添加了一个ImageTextButton的属性集  -->
    <declare-styleable name="LProgressBar">
        <attr name="innerColor" format="color" />
        <attr name="outerColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="innerRadio" format="dimension" />
        <attr name="progressWidth" format="dimension" />
        <attr name="maxSecond" format="integer" />
    </declare-styleable>
</resources>